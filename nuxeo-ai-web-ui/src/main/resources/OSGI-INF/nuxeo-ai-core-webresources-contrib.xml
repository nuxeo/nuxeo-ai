<?xml version="1.0"?>

<component name="org.nuxeo.ai.webui.resources.contrib">
  <require>org.nuxeo.web.ui.resources</require>

  <extension target="org.nuxeo.ecm.platform.WebResources" point="resources">
    <resource name="nuxeo-ai.html" type="import">
      <uri>/ui/nuxeo-ai/nuxeo-ai.html</uri>
    </resource>
  </extension>

  <extension target="org.nuxeo.ecm.platform.WebResources" point="bundles">
    <bundle name="web-ui">
      <resources append="true">
        <resource>nuxeo-ai.html</resource>
      </resources>
    </bundle>
  </extension>

</component>
