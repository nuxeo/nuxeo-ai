<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://www.nuxeo.org/ecm/schemas/transcribe/"
           xmlns:nxtrb="http://www.nuxeo.org/ecm/schemas/transcribe/"
           xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:complexType name="content">
    <xs:sequence>
      <xs:element name="encoding" type="xs:string"/>
      <xs:element name="mime-type" type="xs:string"/>
      <xs:element name="data" type="xs:base64Binary"/>
      <xs:element name="name" type="xs:string"/>
      <xs:element name="length" type="xs:long"/>
      <xs:element name="digest" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>

  <xs:element name="label" type="xs:string"/>
  <xs:element name="raw" type="nxtrb:content"/>
  <xs:element name="normalized" type="nxtrb:content"/>
</xs:schema>
